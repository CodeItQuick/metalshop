{
	"name": "metalshop",
	"version": "0.0.0-dev.42",
	"description": "ready-made app components and microservices",
	"author": "Chase Moskal <chasemoskal@gmail.com>",
	"license": "ISC",
	"type": "module",
	"main": "dist/main.js",
	"module": "dist/main.js",
	"files": [
		"dist"
	],
	"keywords": [
		"auth",
		"components",
		"authorization",
		"authentication",
		"web-components"
	],
	"scripts": {
		"prepare": "run-s clean && run-p compile importmap && run-s mocks test",
		"clean": "rimraf dist && mkdirp dist",
		"importmap": "importly --host=jsdelivr < package.json > dist/importmap.json",
		"compile": "tsc",
		"watch": "tsc -w",
		"mocks": "node dist/build/make-mock-tokens.js",
		"test": "exit 0",
		"start": "http-server"
	},
	"devDependencies": {
		"feather-icons": "^4.26.0",
		"http-server": "^0.12.1",
		"importly": "0.1.0-dev.0",
		"mkdirp": "^1.0.3",
		"npm-run-all": "4.1.5",
		"octicons": "^8.5.0",
		"rimraf": "^3.0.2",
		"typescript": "^3.8.3"
	},
	"dependencies": {
		"authoritarian": "0.0.0-dev.42",
		"commotion": "^0.1.0-dev.0",
		"crosscall": "0.0.6-dev.19",
		"lit-element": "^2.3.0",
		"lit-html": "^1.2.0",
		"menutown": "0.0.1",
		"redcrypto": "0.0.0-dev.4",
		"renraku": "0.0.0-dev.25"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chase-moskal/metalshop.git"
	},
	"bugs": {
		"url": "https://github.com/chase-moskal/metalshop/issues"
	},
	"homepage": "https://github.com/chase-moskal/metalshop#readme"
}
