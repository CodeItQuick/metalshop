{
	"name": "authoritarian-client",
	"version": "0.0.0-dev.29",
	"description": "auth frontend and web components",
	"author": "Chase Moskal <chasemoskal@gmail.com>",
	"license": "ISC",
	"type": "module",
	"main": "dist/main.js",
	"module": "dist/main.js",
	"files": [
		"dist"
	],
	"keywords": [
		"auth",
		"authorization",
		"authentication",
		"web-components"
	],
	"scripts": {
		"prepare": "run-s clean && run-p compile importmap && run-s mocks test",
		"clean": "rimraf dist && mkdirp dist",
		"importmap": "importly --host=jsdelivr < package.json > dist/importmap.json",
		"compile": "tsc",
		"watch": "tsc -w",
		"mocks": "node dist/build/make-mock-tokens.js",
		"test": "exit 0",
		"test-old": "jest --verbose --rootDir dist",
		"start": "http-server"
	},
	"devDependencies": {
		"@types/jest": "^25.1.4",
		"browserify": "16.5.0",
		"envify": "4.1.0",
		"es6-promise": "4.2.8",
		"feather-icons": "^4.26.0",
		"http-server": "^0.12.1",
		"importly": "0.1.0-dev.0",
		"jest": "^25.1.0",
		"mkdirp": "^1.0.3",
		"npm-run-all": "4.1.5",
		"octicons": "^8.5.0",
		"pug-cli": "1.0.0-alpha6",
		"rimraf": "^3.0.2",
		"tsify": "4.0.1",
		"typescript": "^3.8.3",
		"uglify-es": "3.3.9",
		"uglifyify": "5.0.2"
	},
	"dependencies": {
		"authoritarian": "0.0.0-dev.41",
		"commotion": "^0.1.0-dev.0",
		"crosscall": "0.0.6-dev.19",
		"event-decorators": "0.0.0-dev.0",
		"jsonwebtoken": "^8.5.1",
		"lit-element": "^2.2.1",
		"lit-html": "^1.1.2",
		"menutown": "0.0.1",
		"redcrypto": "0.0.0-dev.4",
		"renraku": "0.0.0-dev.25",
		"sassbass": "0.0.2"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chase-moskal/authoritarian-client.git"
	},
	"bugs": {
		"url": "https://github.com/chase-moskal/authoritarian-client/issues"
	},
	"homepage": "https://github.com/chase-moskal/authoritarian-client#readme"
}
